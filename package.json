{
  "name": "cms-kit-sanity",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "lint": "next lint .",
    "lint:fix": "npm run format && npm run lint -- --fix",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "rollout": "./src/rollout-tools/local/cli.mjs",
    "rollout:action": "./src/rollout-tools/demo/cli.mjs",
    "rollout:demo": "node --env-file=.env ./src/rollout-tools/demo/cli.mjs",
    "export-dataset": "sanity dataset export production initial-data.tar.gz",
    "test:content-upload": "sanity dataset create testing; sanity dataset import initial-data.tar.gz testing && sanity dataset delete testing --force"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@focus-reactive/cms-kit-sanity": "0.1.21",
    "@next/env": "14.2.4",
    "@sanity/demo": "2.0.0",
    "@sanity/icons": "3.2.0",
    "@sanity/image-url": "1.0.2",
    "@sanity/preview-url-secret": "1.6.18",
    "@sanity/react-loader": "1.10.4",
    "@sanity/ui": "2.6.1",
    "@sanity/vision": "3.49.0",
    "@tailwindcss/typography": "0.5.13",
    "@vercel/stega": "^0.1.2",
    "classnames": "2.5.1",
    "date-fns": "3.6.0",
    "next": "14.2.4",
    "next-sanity": "9.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rxjs": "7.8.1",
    "sanity": "3.49.0",
    "sanity-plugin-asset-source-unsplash": "3.0.1",
    "server-only": "0.0.1",
    "styled-components": "6.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@types/react": "18.3.3",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "^15.8.0",
    "inquirer": "^10.1.8",
    "ora": "^8.1.0",
    "postcss": "8.4.39",
    "prettier": "3.3.2",
    "prettier-plugin-packagejson": "2.5.0",
    "prettier-plugin-tailwindcss": "0.6.5",
    "tailwindcss": "3.4.4",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.15.0"
  }
}
